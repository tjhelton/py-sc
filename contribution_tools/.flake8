[flake8]
max-line-length = 88
extend-ignore =
    E203,
    W503,
    E501,
    D100,
    D101,
    D102,
    D103,
    D104,
    D105,
    D107,
    D200,
    D205,
    D400,
    D401,
    I201
exclude =
    .git,
    __pycache__,
    venv,
    .venv,
    dump,
    tools,
    build,
    dist,
    *.egg-info
per-file-ignores =
    __init__.py:F401
